<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- crear grupo personalizado -->
    <record id ="spreadsheet_dashboard_otros" model="spreadsheet.dashboard.group">
        <field name="name">Informes</field>
        <field name="sequence">900</field>
    </record>

    <record id="spreadsheet_dashboard_farmaclara_otro" model="spreadsheet.dashboard">
        <field name="name">Tablero Compras</field>
        <field name="spreadsheet_binary_data" type="base64" file="FarmaClara\data\files\compras_dashboard.json"></field>
        <field name="dashboard_group_id" ref="FarmaClara.spreadsheet_dashboard_otros"/>
        <!-- Opcional: restricciones por grupos de usuarios -->
        <field name="group_ids" eval="[Command.link(ref('account.group_account_readonly')), Command.link(ref('account.group_account_invoice'))]"/>
        <field name="sequence">50</field>
    </record>

</odoo>